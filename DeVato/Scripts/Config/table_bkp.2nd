--DROP TABLE
SELECT 'DROP TABLE BKP_01'||TO_CHAR(CURRENT_DATE,'MMYYYY')||'_'||TABLENAME||' IF EXISTS;' AS "--backup tables script--"  FROM TABLE_LIST
UNION ALL
--CREATE TABLE
SELECT 'CREATE TABLE BKP_01'||TO_CHAR(CURRENT_DATE,'MMYYYY')||'_'||TABLENAME||' AS SELECT * FROM '||TABLENAME||';'  FROM TABLE_LIST;